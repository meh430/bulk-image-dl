{
    "name": "Bulk Image Downloader",
    "version": "1.0",
    "description": "A script that downloads images in bulk",
    "manifest_version": 2,
    "permissions": [
        "declarativeContent",
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage",
        "downloads",
        "contextMenus",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": [
            "startup.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    }
}